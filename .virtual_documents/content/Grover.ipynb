def Diffusion(circuit):
    #convert back to the original basis
    for i in range (n):
        circuit.h(i)

    #flip all the qbits
    for i in range (n):
        circuit.x(i)

    # flips the phase of the "1111" state
    circuit.h(n-1)
    circuit.mcx(list(range(n-1)), n-1)
    circuit.h(n-1)

    #return circuit to correct basis
    for i in range (n):
        circuit.x(i)
    for i in range (n):
        circuit.h(i)





